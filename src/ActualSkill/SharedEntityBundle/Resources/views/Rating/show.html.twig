<h1>Rating</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Rating</th>
            <td>{{ entity.rating }}</td>
        </tr>
        <tr>
            <th>Modified_at</th>
            <td>{{ entity.modifiedat|date('Y-m-d H:i:s') }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('rating') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('rating_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('rating_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
