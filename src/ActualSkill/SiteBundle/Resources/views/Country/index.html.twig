<h1>Country list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Iso2</th>
            <th>Iso3</th>
            <th>Iso_numeric</th>
            <th>Fips</th>
            <th>Country</th>
            <th>Capital</th>
            <th>Area</th>
            <th>Population</th>
            <th>Continent</th>
            <th>Currency_code</th>
            <th>Currency_name</th>
            <th>Languages</th>
            <th>Geonameid</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('country_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.iso2 }}</td>
            <td>{{ entity.iso3 }}</td>
            <td>{{ entity.isonumeric }}</td>
            <td>{{ entity.fips }}</td>
            <td>{{ entity.country }}</td>
            <td>{{ entity.capital }}</td>
            <td>{{ entity.area }}</td>
            <td>{{ entity.population }}</td>
            <td>{{ entity.continent }}</td>
            <td>{{ entity.currencycode }}</td>
            <td>{{ entity.currencyname }}</td>
            <td>{{ entity.languages }}</td>
            <td>{{ entity.geonameid }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('country_show', { 'id': entity.id }) }}">show</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

