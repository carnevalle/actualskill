{% extends 'ActualSkillAdminBundle::admin.html.twig' %}

{% block body %}

<h1>Attribute creation</h1>

<form action="{{ path('admin_attribute_create') }}" method="post" {{ form_enctype(form) }}>
    {{ form_widget(form) }}
    <p>
        <button type="submit">Create</button>
    </p>
</form>

<ul class="record_actions">
    <li>
        <a href="{{ path('admin_attributes') }}">
            Back to the list
        </a>
    </li>
</ul>
{% endblock %}